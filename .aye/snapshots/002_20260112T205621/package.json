{
  "name": "vaultguard",
  "version": "1.0.0",
  "description": "Protect Your Secrets, Guard Your Vault - Comprehensive browser-based security analysis tool",
  "main": "dist/background/service-worker.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "package": "npm run build && node scripts/package.js"
  },
  "keywords": [
    "security",
    "scanner",
    "browser-extension",
    "vulnerability",
    "api-keys",
    "secrets"
  ],
  "author": "VaultGuard Team",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.7",
    "prettier": "^3.2.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {}
}